<opml version="1.0">
    <head>
        <title>FeedThing Export</title>
    </head>
    <body>
	{% for f in feeds %}


        <outline text="{{f.display_name|escape}}"
            title="{{f.display_name|escape}}" type="rss"
            xmlUrl="{{f.feedURL|escape}}" htmlUrl="{{f.best_link|escape}}"/>


	{% endfor %}

    </body>
</opml>